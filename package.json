{
  "name": "zlgp",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm --filter @zl-gp/play dev",
    "build": "run-p format build-theme build-core",
    "build-core": "pnpm --filter zl-gp build",
    "build-theme": "pnpm --filter  @zl-gp/theme build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "prepare": "cypress install",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --component'",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write packages/core/ packages/components packages/hooks packages/play packages/utils "
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@vueuse/components": "^13.2.0",
    "@vueuse/core": "^13.2.0",
    "@zl-gp/components": "workspace:*",
    "@zl-gp/hooks": "workspace:*",
    "@zl-gp/play": "workspace:*",
    "@zl-gp/theme": "workspace:*",
    "@zl-gp/utils": "workspace:*",
    "dayjs": "^1.11.13",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@cypress/vue": "^6.0.2",
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.13.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vitest/eslint-plugin": "1.1.25",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.3.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "cypress": "^14.0.1",
    "eslint": "^9.18.0",
    "eslint-plugin-cypress": "^4.1.0",
    "eslint-plugin-vue": "^9.32.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "lodash-es": "^4.17.21",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.4.2",
    "rollup-plugin-sass": "^1.15.2",
    "rollup-plugin-visualizer": "^5.14.0",
    "sass": "^1.84.0",
    "sass-embedded": "^1.83.4",
    "scss": "^0.2.4",
    "start-server-and-test": "^2.0.10",
    "terser": "^5.39.0",
    "typescript": "~5.7.3",
    "unplugin-auto-import": "^19.1.2",
    "unplugin-vue-components": "^28.4.1",
    "vite": "^6.0.11",
    "vite-plugin-dts": "^4.5.0",
    "vite-plugin-vue-devtools": "^7.7.1",
    "vitest": "^3.0.5",
    "vue-router": "^4.5.0",
    "vue-tsc": "^2.2.0"
  }
}